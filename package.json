    "antd": "^5.15.0",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.3",
    "react-query": "^3.39.3",
    "swr": "^2.2.4",
    "zod": "^3.22.4",
    "react-router-dom": "^7.3.0"
  },
