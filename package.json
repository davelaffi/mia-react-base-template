    "@swc/core": "^1.4.17",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "identity-obj-proxy": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-svgr": "3.3.0",
    "vitest": "^1.5.0"
  }
}
